{
  "name": "animakit-expander",
  "version": "2.1.4",
  "description": "AnimakitExpander - React component for the expanding and collapsing of the blocks",
  "main": "index.js",
  "files": [
    "*.md",
    "lib/*.js"
  ],
  "scripts": {
    "clean": "rimraf lib/*.js",
    "build": "npm run clean && babel -s inline -d ./ **/*.es6",
    "prepublishOnly": "npm run build",
    "test": "npm run build && jest"
  },
  "author": "Anna Selezniova",
  "license": "MIT",
  "keywords": [
    "animakit",
    "animation",
    "expander",
    "expand",
    "collapse",
    "react",
    "react-component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/animakit/animakit-expander"
  },
  "homepage": "https://animakit.github.io",
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "jest": "^21.2.1",
    "jsdom": "^11.6.2",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
